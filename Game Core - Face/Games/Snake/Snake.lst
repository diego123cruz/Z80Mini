8213: 44      	    ld b,h
8214: 4D      	    ld c,l
8215: 29      	    add hl,hl
8216: 29      	    add hl,hl
8217: 2C      	    inc l
8218: 09      	    add hl,bc
8219: 224282  	    ld (seed1),hl
821C: 2A4482  	    ld hl,(seed2)
821F: 29      	    add hl,hl
8220: 9F      	    sbc a,a
8221: E62D    	    and %00101101
8223: AD      	    xor l
8224: 6F      	    ld l,a
8225: 224482  	    ld (seed2),hl
8228: 09      	    add hl,bc
